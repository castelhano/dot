  'Multiple Option Select':
    'prefix': 'form_select_multiple'
    'body':'<div class="row g-1">\n
    \t<div class="form-floating mb-1 col-lg">\n
    \t\t<select class="form-control" id="modelos_disponiveis" style="min-height:180px;" multiple>\n
    \t\t\t{% for modelo in modelos %}\n
    \t\t\t<option value="{{modelo.id}}">{{modelo.nome}}</option>\n
    \t\t\t{% endfor %}\n
    \t\t</select>\n
    \t\t<label for="id_modelos_disponiveis">Modelos Disponíveis</label>\n
    \t</div>\n
    \t<div class="col-lg-auto">\n
    \t\t<div class="d-flex justify-content-center d-lg-grid gap-1">\n
    \t\t\t<a class="btn btn-sm btn-secondary pointer" id="select_multiple_add" onclick="addSelected();" style="padding-left: 11px;padding-right: 11px;"><i class="fas fa-angle-right rotate-lg-90"></i></a>\n
    \t\t\t<a class="btn btn-sm btn-secondary pointer" id="select_multiple_add_all" onclick="addAll();"><i class="fas fa-angle-double-right rotate-lg-90"></i></a>\n
    \t\t\t<a class="btn btn-sm btn-secondary pointer" id="select_multiple_remove" onclick="removeSelected();" style="padding-left: 11px;padding-right: 11px;"><i class="fas fa-angle-left rotate-lg-90"></i></a>\n
    \t\t\t<a class="btn btn-sm btn-secondary pointer" id="select_multiple_remove_all" onclick="removeAll();"><i class="fas fa-angle-double-left rotate-lg-90"></i></a>\n
    \t\t</div>\n
    \t</div>\n
    \t<div class="form-floating mb-1 col-lg">\n
    \t\t<select class="form-control" name="modelos" id="id_modelos" style="min-height:180px;" multiple>\n
    \t\t</select>\n
    \t<label for="id_modelos">Modelos Associados</label>\n
    \t</div>\n
    </div>'
  
  'Multiple Option Select Javascript':
    'prefix': 'script_select_multiple'
    'body':'// SET onsubmit="return submitForm();" ON FORM\ndisponiveis = document.getElementById("modelos_disponiveis");\n
    selecionados = document.getElementById("id_modelos");\n
    function submitForm(){options = selecionados.options;for(i=0; i < options.length; i++){options[i].selected = "true";}return true;}\n\n
    function addAll(){selecionados.innerHTML += disponiveis.innerHTML;disponiveis.innerHTML = \'\';}\n
    function removeAll(){disponiveis.innerHTML += selecionados.innerHTML;selecionados.innerHTML = \'\';}\n
    function addSelected(){residual = document.createElement("select");for(var i=0; i < disponiveis.length; i++){if(disponiveis[i].selected){let newOption = new Option(disponiveis[i].text,disponiveis[i].value);selecionados.add(newOption);}else{let newOption = new Option(disponiveis[i].text,disponiveis[i].value);residual.add(newOption);}}disponiveis.innerHTML = residual.innerHTML;}\n
    function removeSelected(){residual = document.createElement("select");for(var i=0; i < selecionados.length; i++){if(selecionados[i].selected){let newOption = new Option(selecionados[i].text,selecionados[i].value);disponiveis.add(newOption);}else{let newOption = new Option(selecionados[i].text,selecionados[i].value);residual.add(newOption);}}selecionados.innerHTML = residual.innerHTML;}'
  
  'Plugin webcam':
    'prefix': 'webcam'
    'body':'<!-- Inserir um botao para iniciar o modal -->\n<!-- <div class="col-auto"><a class="btn btn-light border py-3 px-4" data-bs-toggle="modal" data-bs-target="#modal-webcam"><i class="fas fa-video text-muted"></i></a></div> -->\n{% include "_component/webcam.html" with width=245 height=0 %}'  
  
  
  # JAVASCRIPT
  'Select Populate Ajax':
    'prefix': 'ajax_select'
    'body':'function carregaSetores() {\n\tvar xhttp = new XMLHttpRequest();\n\txhttp.onreadystatechange = function() {\n\t\tif(this.readyState == 4 && this.status == 200){\n\t\t\tif(this.responseText == \'\'){}\n\t\t\telse{\n\t\t\t\tlet obj = JSON.parse(this.responseText);\n\t\t\t\tsetores = document.getElementById("id_setor")\n\t\t\t\tfor(key in obj){setores.innerHTML += \'<option value="\' + obj[key] + \'">\' + key + \'</option>\';}\n\t\t\t}\n\t\t}\n\t};\n\txhttp.open("GET", "url", true);\n\txhttp.send();\n}'
  
  'Script Submit Form':
    'prefix': 'script_submit'
    'body':'function submitForm(){\n\tlet pesquisa = document.getElementById(\'main_search\').value;\n\tif(pesquisa.length > 3 || (pesquisa.length > 1 && pesquisa[0] == \'#\')){return true;}\n\telse{var modal = new bootstrap.Modal(document.getElementById(\'modal-query-alert\'),{keyboard: true});modal.show();return false;}\n}\n'
  
  # PYTHON TEMPLATE SNIPPETS
  'Python IF on Template':
    'prefix': 'tif'
    'body':'{% if ${1:args} %}{% else %}{% endif %}'
  
  'Python FOR on Template':
    'prefix': 'tfor'
    'body':'{% for ${1:arg} in args %}\n{% endfor %}\n'
  
  'Python URL':
    'prefix': 'url'
    'body':"{% url '${1:url}' %}"
  
  'Python Permitions':
    'prefix': 'perm'
