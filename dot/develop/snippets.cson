  # FORMULARIOS
  'Form Imput':
    'prefix': 'form_input'
    'body':'<div class="row g-1">\n\t<div class="form-floating mb-1 col-lg-12">\n\t\t{{ form.nome }}\n\t\t<label for="id_nome">Nome completo</label>\n\t</div>\n</div>'
  
  'Form Select':
    'prefix': 'form_select'
    'body':'<div class="row g-1">\n\t<div class="form-floating mb-1 col-lg-12">\n\t\t<select class="form-select" id="id_nome" name="nome">\n\t\t\t<option value="" selected>---------</option>\n\t\t\t<option value="1">One</option>\n\t\t\t<option value="2">Two</option>\n\t\t</select>\n\t\t<label for="id_nome">modelo</label>\n\t</div>\n</div>'
  
