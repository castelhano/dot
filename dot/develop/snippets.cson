  'Template Edit Form':
    'prefix': '_edit'
    'body':'{% extends "layout/dot.html" %}\n
    {% load static %}\n
    {% block title %}Titulo{% endblock %}\n
    {% block model %}Modulo{% endblock %}\n
    {% block model_menu %}{% include "_component/menu/core.html" %}{% endblock%}\n
    {% block content_fluid %}\n
    <a class="d-none" id="clear" href="url"></a>\n
    <div class="card mt-2">\n
    \t<div class="card-header">\n
    \t\t<ul class="nav nav-tabs card-header-tabs">\n
    \t\t\t<li class="nav-item"><a class="nav-link active" data-bs-toggle="tab" data-bs-target="#base" href="#"><i class="fas fa-list"></i></a></li>\n
    \t\t\t<li class="nav-item"><a class="nav-link" id="back" title="ALT + V" href="url"><i class="fas fa-undo"></i></a></li>\n
    \t\t\t<li class="nav-item dropdown ms-auto">\n
    \t\t\t\t<a class="nav-link dropdown-toggle border text-dark" data-bs-toggle="dropdown" href="#" role="button"></a>\n
    \t\t\t\t<ul class="dropdown-menu">\n
    \t\t\t\t\t<li><a class="dropdown-item pointer" data-bs-toggle="modal" data-bs-target="#history_modal">Historico</a></li>\n
