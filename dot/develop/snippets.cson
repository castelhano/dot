    \t{% csrf_token %}\n
    \t\t<div class="card-body tab-content">\n
    \t\t\t<h5 class="card-title mb-3">Cadastro de Modelo</h5>\n
    \t\t\t<div class="tab-pane fade show active" id="base" role="tabpanel">\n
    \t\t\t\t<div class="row g-1">\n
    \t\t\t\t\t<div class="form-floating mb-lg-1 col-lg">\n
    \t\t\t\t\t{{ form.nome }}\n
    \t\t\t\t\t\t<label for="id_nome">Nome</label>\n
    \t\t\t\t\t</div>\n
