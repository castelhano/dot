  'Chart JS body':
    'prefix': 'chart_body'
    'body':'var ctx = document.getElementById(\'barChart\');\n
    var barChart = new Chart(ctx, {\n
    \t// line bar horizontalBar pie doughnut\n
    \ttype: \'bar\',\n
    \tdata: {\n
    \t\tlabels: {{ categorias|safe }},\n
    \t\tdatasets: [{\n
    \t\t\tlabel: \'Total de Acordos\',\n
    \t\t\tdata: {{ dados }},\n
    \t\t\tbackgroundColor: {{ bgcolors|safe }},\n
    \t\t\tborderColor: {{ bordercolors|safe }},\n
    \t\t\tfill: false,\n
    \t\t\tborderWidth: 1\n
    \t\t}]\n
    \t},\n
    \toptions: {\n
    \t\tscales: {\n
    \t\t\tyAxes: [{\n
    \t\t\t\tticks: {\n
    \t\t\t\t\tbeginAtZero: true\n
    \t\t\t\t}\n
    \t\t\t}]\n
    \t\t},\n
    \t\ttitle: {\n
    \t\t\tdisplay: true,\n
    \t\t\ttext: \'Acordos Mensais\',\n
    \t\t\tfontSize: 18,\n
    \t\t\tposition: \'top\'\n
    \t\t},\n
