# SNIPPETS HTML
'.text.html.basic':
  
  # LAYOUT
  'Template Basic':
    'prefix': '_basic'
    'body':'{% extends "layout/dot.html" %}\n{% block title %}Titulo{% endblock %}\n{% block model %}Modulo{% endblock %}\n{% block model_menu %}\n{% include "_component/menu/core.html" %}\n{% endblock%}\n\n{% block content_fluid %}\n<a class="d-none" id="clear" href="url"></a>\n{% endblock%}\n\n{% block add_script_src %}{% endblock %}\n\n<script>\n{% block add_script %}{% endblock %}\n</script>'
  
  'Template Table':
    'prefix': '_table'
    'body':'{% extends "layout/dot.html" %}\n
    {% load static %}\n
    {% block title %}Titulo{% endblock %}\n
    {% block model %}Modulo{% endblock %}\n
    {% block model_menu %}{% include "_component/menu/core.html" %}{% endblock%}\n\n
    {% block content_fluid %}\n
    <a class="d-none" id="clear" href="url"></a>\n\n
    <div class="card mt-2">\n
    \t<div class="card-header">\n
    \t\t<ul class="nav nav-tabs card-header-tabs">\n
    \t\t\t<li class="nav-item"><a class="nav-link active" data-bs-toggle="tab" data-bs-target="#base" href="#"><i class="fas fa-list"></i></a></li>\n
    \t\t\t<li class="nav-item"><a class="nav-link" data-bs-toggle="tab" data-bs-target="#param" href="#"><i class="fas fa-filter"></i></a></li>\n
    \t\t\t<li class="nav-item"><a class="nav-link pointer" id="search" data-bs-toggle="modal" data-bs-target="#search_modal" onclick="setTimeout(function(){document.getElementById(\'id_pesquisa\').focus();},480);" title="CTRL + /"><i class="fas fa-search"></i></a></li>\n
    \t\t\t{% if perms.app.add_modelo %}\n
    \t\t\t<li class="nav-item"><a class="nav-link border" id="add" href="url" title="ALT+N"><i class="fas fa-plus text-success"></i></a></li>\n
    \t\t\t{% endif %}\n
    \t\t\t<li class="nav-item dropdown ms-auto">\n
    \t\t\t\t<a class="nav-link dropdown-toggle border text-dark" data-bs-toggle="dropdown" href="#" role="button"></a>\n
    \t\t\t\t<ul class="dropdown-menu">\n
    \t\t\t\t\t<li><a class="dropdown-item pointer" id="download" onclick="download_table_as_csv(\'main_table\',\';\')" title="ALT + &darr;">Exportar CSV</a></li>\n
    \t\t\t\t\t{% if perms.core.view_log %}\n
    \t\t\t\t\t<li><a class="dropdown-item pointer" href="{% url \'logs\' %}?target_model=app.modelo&mensagem=DELETE" target="_blank">Log de Exclusões</a></li>\n
    \t\t\t\t\t{% endif %}\n
    \t\t\t\t</ul>\n
    \t\t\t</li>\n
    \t\t</ul>\n
    \t</div>\n
    \t<div class="card-body tab-content">\n
    \t\t<h5 class="card-title mb-3">Modelos <small class="badge bg-primary ms-1">{{modelos.count}}</small></h5>\n
    \t\t<div class="tab-pane fade show active" id="base" role="tabpanel">\n
    \t\t\t<table id="main_table" class="table border table-striped table-hover">\n
    \t\t\t\t<thead>\n
    \t\t\t\t\t<tr>\n
    \t\t\t\t\t\t<th>Nome</th>\n
    \t\t\t\t\t\t{% if perms.app.change_modelo %}\n
    \t\t\t\t\t\t<th>&nbsp;</th>\n
    \t\t\t\t\t\t{% endif %}\n
    \t\t\t\t\t</tr>\n
    \t\t\t\t</thead>\n
    \t\t\t\t<tbody>\n
    \t\t\t\t\t{% for modelo in modelos %}\n
    \t\t\t\t\t<tr>\n
    \t\t\t\t\t\t<td>{{modelo.nome}}</td>\n
    \t\t\t\t\t\t{% if perms.app.change_modelo %}\n
    \t\t\t\t\t\t<td class="text-end fit py-1"><a class="btn btn-sm btn-dark" href="url"><i class="fas fa-pen"></i></a></td>\n
    \t\t\t\t\t\t{% endif %}\n
    \t\t\t\t\t</tr>\n
    \t\t\t\t\t{% empty %}\n
    \t\t\t\t\t<tr>\n
    \t\t\t\t\t\t<td colspan="2">Nenhum registro a exibir</td>\n
    \t\t\t\t\t</tr>\n
    \t\t\t\t\t{% endfor %}\n
    \t\t\t\t</tbody>\n
    \t\t\t</table>\n
    \t\t</div>\n
    \t\t<div class="tab-pane fade" id="param" role="tabpanel">param</div>\n
    \t</div>\n
    </div>\n\n    
    <div class="modal fade" id="search_modal" tabindex="-1">\n
    \t<div class="modal-dialog">\n
    \t\t<div class="modal-content">\n
    \t\t\t<div class="row">\n
    \t\t\t\t<div class="col"><h5 class="ms-3 mt-3">Pesquisa</h5></div>\n
    \t\t\t\t<div class="col-auto pe-4 pt-2"><button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button></div>\n
    \t\t\t</div>\n
    \t\t\t<div class="modal-body">\n
    \t\t\t\t<form id="app_form" action="url" method="POST" autocomplete="off">\n
    \t\t\t\t{% csrf_token %}\n
    \t\t\t\t\t<div class="row g-1">\n
    \t\t\t\t\t\t<div class="form-floating mb-1 col-lg-12">\n
    \t\t\t\t\t\t\t<input type="text" class="form-control" name="pesquisa" value="" id="id_pesquisa" placeholder="">\n
    \t\t\t\t\t\t\t<label for="id_pesquisa">Termo pesquisa</label>\n
    \t\t\t\t\t\t</div>\n
    \t\t\t\t\t</div>\n
    \t\t\t\t</form>\n
    \t\t\t</div>\n
    \t\t</div>\n
    \t</div>\n
    </div>\n
    {% endblock%}\n\n

    {% block add_script_src %}\n
    <script src="{% static \'js/table_to_csv.js\' %}"></script>\n
    {% endblock %}'

